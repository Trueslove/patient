}
.home-answer-name {
  font-size: 32rpx;
  color: #313131;
  margin-bottom: 5rpx;
}
.home-answer-btn {
  width: 93rpx;
  height: 43rpx;
  line-height: 43rpx;
  background: #FFDE9E;
  border-radius: 22rpx;
  font-size: 24rpx;
  color: #B17400;
}