}

.home-c-fz {
  height: 334rpx;
  background: linear-gradient(119deg, #EEF8FF 0%, #CBE8FD 100%);
  border-radius: 14rpx;
  padding: 0 20rpx 21rpx 24rpx;
  position: relative;
}
.home-fz-info-wap {
  display: flex;
}
.home-c-fz-ren {
  width: 256rpx;
  height: 346rpx;
  margin-top: -37rpx;
}

.home-fz-i-title {
  font-size: 36rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 500;
  line-height: 50rpx;
  color: #000000;
  margin: 25rpx 0 20rpx 0;
}
.home-c-fz-info {
  margin-left: -17rpx;
}
.home-fz-i-cell {
  display: flex;
  margin-top: 10rpx;
}
.home-fz-cell-label {
  height: 32rpx;
  background: #FF9436;
  border-radius: 5rpx;
  padding: 0 7rpx;
  font-size: 22rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 32rpx;
  color: #FFFFFf;
}
.home-fz-cell-value {
  font-size: 26rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 35rpx;
  color: #5B5B5B;
  margin-left: 18rpx;
}
.home-fz-v-img {
  width: 31rpx;
  height: 31rpx;
  background: rgba(0, 0, 0, 0);
  border: 2rpx solid #FFFFFF;
  box-shadow: 0px 1rpx 6rpx rgba(0, 0, 0, 0.16);
  border-radius: 50%;
  margin-right: 12rpx;
}
.home-fz-v-flex {
  display: flex;
  align-items: center;
}

.home-fz-i-btn {
  position: absolute;
  bottom: 21rpx;
  height: 60rpx;
  border-radius: 30rpx;
  line-height: 60rpx;
  background: #ffffff;
  width: 649rpx;
  margin-top: -21rpx;
  z-index: 111;
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
}
.home-fz-i-btn::before {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  top: -20rpx;
  left: 84rpx;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid transparent;
  border-bottom: 20rpx solid #ffffff;
}
.home-fz-flex {
  display: flex;
  justify-content: center;
  align-items: center;
}
.home-fz-left {
  flex: 1;
  color: #0086E4;
}
.home-fz-right {
  width: 228rpx;
  height: 60rpx;
  border-radius: 0 30rpx 30rpx 0; 
  background: linear-gradient(180deg, #0086E4 0%, #0070BE 100%);
  color: #ffffff;
  position: relative;
}
.home-fz-right::after{
  content: '';
  position: absolute;
  left: 0;
  width: 0;
  height: 0;
  border-top: 60rpx solid #ffffff;
  border-right: 20rpx solid transparent;
}
.home-fz-b-tz {
  width: 26rpx;
  height: 23rpx;
  margin-right: 11rpx;
}
.home-fz-b-arrow {
  width: 10rpx;
  height: 18rpx;
  margin-left: 13rpx;
}


.home-children {
  margin-top: 38rpx;
}
.home-children-tit {
  font-size: 36rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 50rpx;
  color: #000000;
  position: relative;
  z-index: 1;
}
.home-children-tit::before {
  content: "";
  width: 318rpx;
  height: 9rpx;
  background: rgba(200, 226, 255, .8);
  position: absolute;
  bottom: 9rpx;
  left: 0;
  z-index: -1;
}
.home-children-info {
  background: #EFF7FF;
  position: relative;
  border-radius: 14rpx;
  margin-top: 33rpx;
}
.home-children-bj {
  position: absolute;
  right: 0;
  top: 0;
  width: 345rpx;
  height: 201rpx;
}
.home-children-info {
  padding: 26rpx 22rpx;
}
.home-child-cell {
  margin-bottom: 32rpx;
}
.home-child-cell:last-child {
