.fill-visit {
    padding: 128rpx 0 150rpx 0;
}

.online-re-header {
    height: 128rpx;
    background: #0087E4;
    overflow: hidden;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1000;
}

.online-re-h-tit {
    margin: 52rpx 0 0 88rpx;
    line-height: 52rpx;
    font-size: 36rpx;
    font-family: Source Han Sans SC;
    font-weight: 400;
    color: #FFFFFF;
}


.fill-step {
    height: 115rpx;
    background: #FFFFFF;
    box-shadow: 0px 2rpx 12rpx rgba(0, 0, 0, 0.08);
    display: flex;
    justify-content: center;
    align-items: center;
}

.step-active .fill-step-num {
    background: #0087E4;
}

.step-active {
    color: #0087E4 !important;
}

.fill-step-cell {
    width: 60rpx;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    text-align: center;
    font-size: 24rpx;
    font-family: Arvo;
    font-weight: 400;
    color: #939393;
    line-height: 35rpx;
    align-items: center;
}

.fill-step-num {
    width: 42rpx;
    height: 42rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 42rpx;
    color: #FFFFFF;
    margin-bottom: 5rpx;
    background-color: #BFBFBF;
}

.fill-step-cell-zp {
    position: relative;
    padding: 0 186rpx;
}

.fill-step-cell-zp::after {
    content: "";
    position: absolute;
    width: 90rpx;
    height: 2rpx;
    background-color: #BFBFBF;
    right: 52rpx;
}

.fill-step-cell-zp::before {
    content: "";
    position: absolute;
    width: 90rpx;
    height: 2rpx;
    background-color: #BFBFBF;
    left: 52rpx;
}



